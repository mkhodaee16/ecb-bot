{
  "version": 2,
  "framework": "flask",
  "builds": [
    {
      "source": "app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    },
    {
      "source": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "app.py"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}